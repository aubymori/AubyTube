{% import "core/macros.twig" as core %}
<!DOCTYPE html>
<html class="no-focus-outine">
    <head>
        <title>404 Not Found</title>
        {{ core.css("www-core") }}
        {{ core.css("www-error") }}
        {{ core.css("www-pageframe") }}
    </head>
    <body>
        <div id="error-page">
            <div id="error-page-content">
                <img src="/ats/imgbin/image-hh-404.png" alt="" id="error-page-hh-illustration">
                <p>This page isn't available. Sorry about that.</p>
                <p>Try searching for something else.</p>
                <div id="yt-masthead">
                    <a href="/" id="logo-container" title="AubyTube home">
                        <img src="/ats/imgbin/pixel.gif" alt="AubyTube home" id="logo">
                    </a>
                    <form action="/results" id="masthead-search" class="search-form consolidated-form" onsubmit="if (_gel('masthead-search-term').value == '') return false;">
                        <button id="search-btn" class="yt-uix-button yt-uix-button-size-default yt-uix-button-default search-btn-component search-button" type="submit" onclick="if (_gel('masthead-search-term').value == '') return false; _gel('masthead-search').submit(); return false;;return true;" tabindex="2">
                            <span class="yt-uix-button-content">Search</span>
                        </button>
                        <div id="masthead-search-terms" class="masthead-search-terms-border">
                            <label>
                                <input name="search_query" type="text" id="masthead-search-term" class="search-term yt-uix-form-input-bidi" autocomplete="off" spellcheck="false" style="outline: currentcolor none medium;" title="Search">
                            </label>
                        </div>
                    </form>
                </div>
            </div>
            <span id="error-page-vertical-align"></span>
        </div>
        {{ core.js("www-notfound", "www-notfound/www-notfound") }}
        <script>
            yt.setConfig({'SBOX_JS_URL': "\/ats\/jsbin\/www-searchbox.js",'SBOX_SETTINGS': {"REQUEST_DOMAIN": "us", "REQUEST_LANGUAGE": "en", "HAS_ON_SCREEN_KEYBOARD": false},'SBOX_LABELS': {"SUGGESTION_DISMISS_LABEL": "Dismiss", "SUGGESTION_DISMISSED_LABEL": "Suggestion dismissed"}});
            yt.www.notfound.init();
        </script>
    </body>
</html>