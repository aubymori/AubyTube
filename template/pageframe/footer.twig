<div id="footer">
    <div id="footer-main">
        <div id="footer-logo">
            <a href="/" class="spf-link" title="AubyTube home">
                <span class="footer-logo-icon yt-sprite"></span>
            </a>
        </div>
        <ul class="pickers yt-uix-button-group">
            <li>
                <button id="yt-picker-language-button" disabled="" title="This feature is unavailable" class="yt-uix-tooltip yt-uix-button yt-uix-button-size-default yt-uix-button-default yt-uix-button-has-icon">
                    <span class="yt-uix-button-icon-wrapper">
                        <span class="yt-uix-button-icon yt-uix-button-icon-footer-language yt-sprite"></span>
                    </span>
                    <span class="yt-uix-button-content">
                        <span class="yt-picker-button-label">Language: </span>
                        English
                    </span>
                    <span class="yt-uix-button-arrow yt-sprite"></span>
                </button>
            </li>
            <li>
                <button id="yt-picker-country-button" disabled="" title="This feature is unavailable" class="yt-uix-tooltip yt-uix-button yt-uix-button-size-default yt-uix-button-default">
                    <span class="yt-uix-button-content">
                        <span class="yt-picker-button-label">Country: </span>
                        Worldwide
                    </span>
                    <span class="yt-uix-button-arrow yt-sprite"></span>
                </button>
            </li>
        </ul>
        {% apply spaceless %}
        <a href="/feed/history" class="yt-uix-button footer-history yt-uix-sessionlink yt-uix-button-default yt-uix-button-size-default yt-uix-button-has-icon">
            <span class="yt-uix-button-icon-wrapper">
                <span class="yt-uix-button-icon yt-uix-button-icon-footer-history yt-sprite"></span>
            </span>
            <span class="yt-uix-button-content">History</span>
        </a>
        {% endapply %}
        {% apply spaceless %}
        <a href="/at/help" class="yt-uix-button yt-uix-button-size-default yt-uix-button-default yt-uix-button-has-icon yt-uix-button-reverse">
            <span class="yt-uix-button-icon-wrapper">
                <span class="yt-uix-button-icon yt-uix-button-icon-questionmark yt-sprite"></span>
            </span>
            <span class="yt-uix-button-content">Help</span>
        </a>
        {% endapply %}
    </div>
    <div id="footer-links">
        <ul id="footer-links-primary">
            <li>
                <a href="/at/about">About</a>
            </li>
            <li>
                <a href="/at/copyright">Copyright</a>
            </li>
            <li>
                <a href="/at/developers">Developers</a>
            </li>
            <li>
                <a href="//github.com/aubymori/AubyTube">GitHub</a>
            </li>
        </ul>
        <ul id="footer-links-secondary">
            <li>
                <a href="/t/terms">Terms</a>
            </li>
            <li>
                <a href="/at/privacy">Privacy</a>
            </li>
            <li>
                <a href="//github.com/aubymori/AubyTube/issues/new">Send feedback</a>
            </li>
            <li>
                <span class="copyright">&copy; 2022 aubymori</span>
            </li>
        </ul>
    </div>
</div>